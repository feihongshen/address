	<changeSet id="xushiju-2014-07-21-04" author="xushiju">
		<sql>
			create table `SCHEDULED_TASKS` (
			`ID` int(22) primary key not null auto_increment comment
			'id',
			`TASK_TYPE` varchar(50) not null,
			`STATUS` int(2) not null,
			`REFERENCE_TYPE` varchar(50),
			`REFERENCE_ID` varchar(32),
			`FIRE_TIME` DATETIME,
			`COMPLETED_TIME` DATETIME,
			`CREATED_AT` DATETIME,
			`TRY_COUNT` int (10)
			) ENGINE=InnoDB DEFAULT CHARSET=utf8 COLLATE=utf8_bin;

			CREATE INDEX
			`IDX_TASKS_STATUS_FIRE_TIME` ON `SCHEDULED_TASKS`
			(
			`STATUS`,
			`FIRE_TIME`
			);

			CREATE INDEX
			`IDX_TASKS_STATUS_TYPE_FIRE_TIME` ON
			`SCHEDULED_TASKS`
			(
			`STATUS`,
			`TASK_TYPE`,
			`FIRE_TIME`
			);

			CREATE
			INDEX `IDX_TASKS_REFERENCE_ID` ON
			`SCHEDULED_TASKS`
			(
			`REFERENCE_ID`
			);
		</sql>
	</changeSet>



