	<!-- 添加数字地图功能 新增脚本 -->
	<!-- 创建‘粗’地址表 -->
	<changeSet id="songkaojun-2015-04-07-03" author="songkaojun">
		<sql>
			CREATE TABLE RAW_ADDRESS (
			`ID` int(12) NOT NULL AUTO_INCREMENT,
			`NAME` varchar(50) COLLATE
			utf8_bin NOT NULL,
			`OLD_NAME` varchar(50) COLLATE utf8_bin DEFAULT NULL,
			`STATUS` int(2) NOT NULL,
			`ADDRESS_TYPE_ID` int(2) DEFAULT NULL,
			`ADDRESS_LEVEL` int(2) DEFAULT NULL,
			`PARENT_ID` int(12)
			DEFAULT NULL,
			`PATH` varchar(100) COLLATE utf8_bin DEFAULT NULL,
			`INDEXED` int(1) DEFAULT NULL,
			`CREATION_TIME` datetime DEFAULT NULL,
			PRIMARY KEY (`ID`),
			KEY `IDX_ADDRESS_NAME` (`NAME`),
			KEY
			`IDX_ADDRESS_PARENT_ID` (`PARENT_ID`)
			) ENGINE=InnoDB AUTO_INCREMENT=25596 DEFAULT CHARSET=utf8
			COLLATE=utf8_bin;
		</sql>
	</changeSet>

	<!-- 创建‘粗’地址权限表 -->
	<changeSet id="songkaojun-2015-04-07-04" author="songkaojun">
		<sql>
			CREATE TABLE RAW_ADDRESS_PERMISSIONS (
			`ID` int(12) NOT NULL AUTO_INCREMENT,
			`RAW_ADDRESS_ID`
			int(12) NOT NULL,
			`CUSTOMER_ID` int(12) DEFAULT NULL,
			PRIMARY KEY (`ID`),
			KEY
			`IDX_ADDRESS_PERMISSION_ADDRESS_ID` (`RAW_ADDRESS_ID`)
			) ENGINE=InnoDB AUTO_INCREMENT=54190
			DEFAULT CHARSET=utf8 COLLATE=utf8_bin;
		</sql>
	</changeSet>

	<!-- 创建‘粗’配送站点表 -->
	<changeSet id="songkaojun-2015-04-07-05" author="songkaojun">
		<sql>
			CREATE TABLE RAW_DELIVERY_STATIONS (
			`ID` int(12) NOT NULL AUTO_INCREMENT,
			`NAME` varchar(20)
			COLLATE utf8_bin NOT NULL,
			`COORDINATE` varchar(20000) COLLATE utf8_bin DEFAULT NULL,
			`MAPCENTER_LNG` decimal(50,8) DEFAULT NULL,
			`MAPCENTER_LAT` decimal(50,8) DEFAULT NULL,
			`UID`
			varchar(50) COLLATE utf8_bin DEFAULT NULL,
			`EXTERNAL_ID` int(12) DEFAULT NULL,
			`CUSTOMER_ID`
			int(12) NOT NULL,
			`STATUS` int(2) NOT NULL,
			`CREATION_TIME` datetime DEFAULT NULL,
			PRIMARY KEY
			(`ID`),
			UNIQUE KEY `IDX_DELIVERY_STATION_EXTERNAL_ID` (`EXTERNAL_ID`,`CUSTOMER_ID`)
			) ENGINE=InnoDB
			AUTO_INCREMENT=218 DEFAULT CHARSET=utf8 COLLATE=utf8_bin;
		</sql>
	</changeSet>

	<!-- 创建‘粗’配送站点规则表 -->
	<changeSet id="songkaojun-2015-04-07-06" author="songkaojun">
		<sql>
			CREATE TABLE RAW_DELIVERY_STATION_RULES (
			`ID` int(12) NOT NULL AUTO_INCREMENT,
			`RULE`
			varchar(100) COLLATE utf8_bin DEFAULT NULL,
			`RULE_EXPRESSION` varchar(1000) COLLATE utf8_bin
			DEFAULT NULL,
			`RULE_TYPE` int(2) NOT NULL,
			`RAW_ADDRESS_ID` int(12) NOT NULL,
			`RAW_DELIVERY_STATION_ID` int(12) NOT NULL,
			`CREATION_TIME` datetime DEFAULT NULL,
			PRIMARY KEY
			(`ID`),
			KEY `IDX_DS_RULE_ADDRESS_ID` (`RAW_ADDRESS_ID`)
			) ENGINE=InnoDB AUTO_INCREMENT=12691
			DEFAULT CHARSET=utf8 COLLATE=utf8_bin;
		</sql>
	</changeSet>

	<!-- 创建关键词后缀表 -->
	<changeSet id="songkaojun-2015-04-08-01" author="songkaojun">
		<sql>
			CREATE TABLE KEYWORD_SUFFIX (
			`ID` int(12) NOT NULL AUTO_INCREMENT,
			`CUSTOMER_ID` int(12)
			DEFAULT NULL,
			`NAME` varchar(50) COLLATE utf8_bin NOT NULL,
			PRIMARY KEY (`ID`)
			) ENGINE=InnoDB
			AUTO_INCREMENT=241 DEFAULT CHARSET=utf8 COLLATE=utf8_bin;
		</sql>
	</changeSet>


	<!-- 为配送站点表添加“坐标”，“中心经度”，“中心维度”，“UID”字段，并修改EXTERNAL_ID字段为可空 -->
	<changeSet id="songkaojun-2015-04-07-07" author="songkaojun">
		<sql>
			ALTER TABLE DELIVERY_STATIONS ADD COLUMN `COORDINATE` varchar(20000) NULL AFTER `NAME`;
			ALTER
			TABLE DELIVERY_STATIONS ADD COLUMN `MAPCENTER_LNG` decimal(50,8) NULL DEFAULT NULL AFTER
			`COORDINATE`;
			ALTER TABLE DELIVERY_STATIONS ADD COLUMN `MAPCENTER_LAT` decimal(50,8) NULL
			DEFAULT
			NULL AFTER
			`MAPCENTER_LNG`;
			ALTER TABLE DELIVERY_STATIONS ADD COLUMN `UID` varchar(50)
			CHARACTER SET
			utf8 COLLATE utf8_bin NULL
			DEFAULT NULL AFTER `MAPCENTER_LAT`;

			ALTER TABLE
			DELIVERY_STATIONS MODIFY
			COLUMN `EXTERNAL_ID` int(12) NULL AFTER `UID`;
		</sql>
	</changeSet>

	<!-- 创建地址信息详细表，便于查询 此表中的数据是从raw_address表中同步过来的 -->
	<changeSet id="songkaojun-2015-04-21-01" author="songkaojun">
		<sql>
			CREATE TABLE ADDRESS_DETAIL (
			`id` int(11) NOT NULL AUTO_INCREMENT,
			`province` varchar(255)
			DEFAULT NULL,
			`city` varchar(255) DEFAULT NULL,
			`district` varchar(255) DEFAULT NULL,
			`address_id1`
			int(20) DEFAULT NULL,
			`address_name1` varchar(255) DEFAULT NULL,
			`address_id2` int(20) DEFAULT
			NULL,
			`address_name2` varchar(255) DEFAULT NULL,
			`address_id3` int(255) DEFAULT NULL,
			`address_name3` varchar(255) DEFAULT NULL,
			`delivery_station_name` varchar(255) DEFAULT NULL,
			`customer_id` int(11) DEFAULT NULL,
			PRIMARY KEY (`id`)
			) ENGINE=InnoDB AUTO_INCREMENT=142942
			DEFAULT CHARSET=utf8;
		</sql>
	</changeSet>

